{
  "compilerOptions": {
  

  
    "target": "ESNext",                                  
   

    
    "module": "NodeNext",                                
    "moduleResolution": "nodenext",                     
                
    "rootDir": "./src",                                   
                                    
    "types": ["@prisma/client"],                                      
                                    
    "outDir": "dist",                                 
                
    "esModuleInterop": true,                            
                           
    "forceConsistentCasingInFileNames": true,            

    
    "strict": true,                                     
                     
    "skipLibCheck": true                                 
  },
  "include": ["src/**/*.ts"],
  "exclude":["node_modules"]
}
